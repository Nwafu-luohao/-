<view class="da_view">
  <image src="{{detail.meeting_img}}" class="image"></image>
</view>

<view class="da_view">
  <view class="table">
    <view class="tr bg-w">
      <view class="th2">会议室详情</view>
    </view>


    <view class="td0">
      <view class="td0_2">会议室名:</view>
      <view class="td2">{{detail.name}}</view>
    </view>


    <view class="td0">
      <view class="td0_2">管理员电话：</view>
      <view class="td2">{{detail.phoneNumber}}</view>
    </view>


    <view class="td0">
      <view class="td0_2">地址编号：</view>
      <view class="td2">{{detail.addressNumber}}</view>
    </view>


    <view class="td0">
      <view class="td0_2">经度：</view>
      <view class="td2">{{detail.Jingwei}}</view>
    </view>

    <view class="td0">
      <view class="td0_2">纬度：</view>
      <view class="td2">{{detail.Jingwei2}}</view>
    </view>


    <view class="td0">
      <view class="td0_2">地址：</view>
      <view class="td2">{{detail.address}}</view>
    </view>


    <!-- <view class="td0 td0_2_xiugai1" bindtap="xiugaiyuyuexinxi">
      <view class="td0_2_xiugai" bindtap="xiugaiyuyuexinxi" data-_id="{{detail._id}}">修改预约信息</view>
    </view> -->
    
  </view>
  <button class="btn1" type="primary" bindtap="xiugaixinxi" data-_id="{{detail._id}}">修改信息</button>
  <button class="btn2" type="warn" bindtap="shanchuhuiyishi" data-_id="{{detail._id}}">删除会议室</button>
  <view class="mouwei_view"></view>
</view>