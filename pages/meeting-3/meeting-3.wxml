<view class="view1" style=" border: 10rpx solid rgb(180, 255, 180);">
  <view class="view2"  style="display: flex; align-items: center;">
    <view class="view3" >
      <view class="zhuangtai" style="font-size: 1.5em; font-weight: 900; color: honeydew; margin-left: 40rpx;">预约状态：</view>
    </view>
    <view class="view4">
      <image src="/images/chenggong.png" class="image"></image>
    </view>
    <view class="view4">
      <view class="view6" style="color: rgb(0, 211, 0); margin-left: 25rpx;">已预约</view>
    </view>
